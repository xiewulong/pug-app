  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiewulong/pug-app/issues"
  },
  "homepage": "https://github.com/xiewulong/pug-app#readme",
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^1.12.4",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "normalize.css": "^8.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-sprites": "^4.2.1",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.0.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0"
  }
