{
  "name": "pug-app",
  "version": "0.0.1",
  "description": "App frame on pug",
  "private": true,
  "scripts": {
    "build": "npm run webpack -- --display-error-details --mode=production --optimize-minimize",
    "release": "npm run build",
    "test": "npm start",
    "start": "npm run webpack -- --debug --mode=development --output-pathinfo --watch",
    "webpack": "webpack --colors --devtool sourcemap --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiewulong/pug-app.git"
  },
  "keywords": [
    "pug",
    "app"
  ],
